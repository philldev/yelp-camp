<%- include("./partials/header") %>
    <%- include("./partials/form-head") %>
        <h1 class="text-center">Login</h1>
        <form action="/login" method="post">
            <div class="form-group">
                <label for="username">Username</label>
                <input class="form-control" type="text" id="username" name="username" placeholder="eg. John123">
            </div>
            <div class="form-group">
                <label for="password">Password</label>
                <input class="form-control" type="password" id="password" name="password" placeholder="Your password">
            </div>
            <div class="form-group">
                <button class="btn btn-primary btn-block">Login</button>
            </div>
            <a class="text-primary" href="/campgrounds">Go back</a>
        </form>
    <%- include("./partials/form-foot") %>
<%- include("./partials/footer") %>